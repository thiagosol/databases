[mysqld]
# Tuning geral
innodb_buffer_pool_size=128M
innodb_log_file_size=64M
innodb_flush_log_at_trx_commit=2
innodb_flush_method=O_DIRECT
query_cache_type=0
query_cache_size=0
max_connections=100
tmp_table_size=64M
max_heap_table_size=64M
table_open_cache=400
thread_cache_size=50

slow_query_log=1
long_query_time=1

performance_schema=ON
